# Helix Regulatory Platform - Environment Variables Template
# Copy to .env and fill in your values

# Database Configuration
DATABASE_URL=postgresql://username:password@hostname:port/database_name

# Application Environment
NODE_ENV=production
PORT=5000

# Optional: External API Keys (if needed for regulatory data sources)
# FDA_API_KEY=your_fda_api_key_here
# EMA_API_KEY=your_ema_api_key_here

# Optional: Email Service (if using SendGrid for newsletters)
# SENDGRID_API_KEY=your_sendgrid_api_key_here

# Optional: AI Service (if using external AI APIs)
# ANTHROPIC_API_KEY=your_anthropic_api_key_here